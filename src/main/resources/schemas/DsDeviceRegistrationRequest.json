{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "DsDeviceRegistrationRequest",
  "javaType": "com.t1t.t1c.ds.DsDeviceRegistrationRequest",
  "type": "object",
  "properties": {
    "activated": {
      "type": "boolean"
    },
    "uuid": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "derEncodedPublicKey": {
      "type": "string"
    },
    "os": {
      "type": "object",
      "$ref": "DsOs.json"
    },
    "desktopApplication": {
      "type": "object",
      "$ref": "DsDesktopApplication.json"
    },
    "clientInfo": {
      "type": "object",
      "$ref": "DsClientInfo.json"
    },
    "containerStates": {
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "GclContainerInfo.json"
      }
    },
    "proxyDomain": {
      "type": "string"
    },
    "namespace": {
      "type": "string"
    }
  }
}