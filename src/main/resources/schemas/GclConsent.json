{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "object",
  "$id": "GclConsent",
  "javaType": "com.t1t.t1c.core.GclConsent",
  "properties": {
    "title": {
      "type": "string"
    },
    "text": {
      "type": "string"
    },
    "days": {
      "type": "integer"
    },
    "alert_level": {
      "type": "string",
      "enum": [
        "information",
        "question",
        "warning",
        "error"
      ]
    },
    "alert_position": {
      "type": "string",
      "enum": [
        "standard",
        "center",
        "left",
        "right",
        "top",
        "top_left",
        "top_right",
        "bottom",
        "bottom_left",
        "bottom_right"
      ]
    },
    "type": {
      "type": "string",
      "enum": [
        "reader",
        "file_exchange"
      ]
    },
    "timeout": {
      "type": "integer"
    }
  }
}